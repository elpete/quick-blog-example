{
    "name":"quick-blog-example",
    "version":"1.0.0",
    "author":"Eric Peterson <eric@ortussolutons.com>",
    "type":"mvc",
    "keywords":"quick,orm,example,coldbox",
    "repository":{
        "type":"git",
        "url":"https://github.com/elpete/quick-blog-example"
    },
    "shortDescription":"An example blog built with ColdBox and Quick",
    "ignore":[],
    "devDependencies":{
        "testbox":"^3.0.0",
        "commandbox-dotenv":"*",
        "commandbox-cfconfig":"*",
        "commandbox-cfformat":"*"
    },
    "installPaths":{
        "coldbox":"coldbox/",
        "testbox":"testbox/",
        "quick":"modules/quick/"
    },
    "dependencies":{
        "coldbox":"^5.0.0",
        "quick":"be"
    },
    "scripts":{},
    "cfmigrations":{
        "schema":"${DB_SCHEMA}",
        "connectionInfo":{
            "password":"${DB_PASSWORD}",
            "connectionString":"${DB_CONNECTIONSTRING}",
            "class":"${DB_CLASS}",
            "username":"${DB_USER}",
            "bundleName":"${DB_BUNDLENAME}",
            "bundleVersion":"{DB_BUNDLEVERSION}"
        },
        "defaultGrammar":"MySQLGrammar@qb",
        "migrationsDirectory": "resources/database/migrations"
    }
}
